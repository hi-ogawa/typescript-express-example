{
  "scripts": {
    "build": "tsc --sourceMap",
    "build:watch": "tsc --sourceMap --watch",
    "build:clean": "rm -rf build",
    "routes": "ts-node -T src/scripts/make-routes src/routes src/routes.ts",
    "db": "ts-node -T src/scripts/db.ts",
    "console": "node --experimental-repl-await",
    "server": "node build/server.js",
    "server:watch": "nodemon build/server.js",
    "test": "NODE_ENV=test mocha",
    "test:watch": "npm run test -- --watch",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "devDependencies": {
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.12",
    "@types/mocha": "^8.2.2",
    "@types/morgan": "^1.9.2",
    "@types/node": "^15.14.0",
    "@types/pg": "^8.6.0",
    "@types/supertest": "^2.0.11",
    "mocha": "^9.0.1",
    "nodemon": "^2.0.9",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.19",
    "supertest": "^6.1.3",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "commander": "^7.2.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "knex": "^0.95.6",
    "morgan": "^1.10.0",
    "pg": "^8.6.0"
  }
}
